- name: KY pension exclusion unit test 1
  period: 2021
  input:
    people:
      person1:
        is_tax_unit_head: true        
        taxable_pension_income: 1_000
      person2:
        is_tax_unit_spouse: true
        taxable_pension_income: 7_000
    households:
      household:
        members: [person1, person2]
        state_code: KY
  output:
    ky_pension_exclusion: [1_000, 7_000]

